declare i32 @putchar(i32)

define i32 @main() {
entry:
  %y = alloca i32, align 4
  %x = alloca i32, align 4
  store i32 5, ptr %x, align 4
  %x1 = load i32, ptr %x, align 4
  %addtmp = add i32 %x1, 3
  store i32 %addtmp, ptr %y, align 4
  %y2 = load i32, ptr %y, align 4
  %x3 = load i32, ptr %x, align 4
  %cmptmp = icmp sgt i32 %y2, %x3
  br i1 %cmptmp, label %then, label %else

then:                                             ; preds = %entry
  %0 = call i32 @putchar(i8 79)
  %1 = call i32 @putchar(i8 75)
  br label %ifcont

else:                                             ; preds = %entry
  %2 = call i32 @putchar(i8 66)
  %3 = call i32 @putchar(i8 65)
  %4 = call i32 @putchar(i8 68)
  br label %ifcont

ifcont:                                           ; preds = %else, %then
  %iftmp = phi i32 [ 0, %then ], [ 1, %else ]
  ret i32 %iftmp
}